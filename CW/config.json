{
	"appModes":
	[
		{
			"modeName": "circleSpawn",
			"onStartRunEval": [
				" createjs.Ticker.framerate = 15; ",
				" INFO.maxCircleNum = 40; "				
			],
			"onFrameMoveEval": [
				" /* This gets called on every frame/tick - create circle.. */ ",
				" if ( INFO.frameCount === undefined ) INFO.frameCount = 0; ",
				" else INFO.frameCount++; ",
				" ",
				" var colorTeamList = [ 'blue', 'red', 'gray', 'green' ]; ",
				" ",
				" if ( ( StageManager.stage.children.length <= INFO.maxCircleNum ) && ( INFO.frameCount % 30 ) === 0 ) { ",
				"   var propJson = { ",
				"     color: Util.getRandomInList( colorTeamList ), ",
				"     startPosition: AppUtil.getPosition_Center() ",
				"   };  ",
				"   ",
				"   var container = CircleManager.createCircleItem( propJson ); ",
				"   container.itemData.onFrameMove = container => { ", 
				"     if ( !container.itemData.innerCircle && container.itemData.age > 10 ) CircleManager.addInnerCircle_setInItemData( container, { width_half: 4, color: Util.getRandomColorHex() } ); ",
				"     MovementHelper.moveNext( container ); ",
				"   } ",				
				" } "
			]	
		},
		{
			"modeName": "portalTwo",
			"onStartRunEval": [
				" createjs.Ticker.framerate = 15; ",
				" ",
				" for( var i = 0; i < 2; i++ ) PortalManager.createPortalItem(); "
			],
			"onFrameMoveEval": [ ]	
		}
	],
